    <listViews>
        <fullName>Closed_Order</fullName>
        <columns>NAME</columns>
        <columns>Account__c</columns>
        <columns>Opportunity__c</columns>
        <columns>ptt_Status__c</columns>
        <columns>sf42_Order_Date__c</columns>
        <columns>sf42_Delivery_Date__c</columns>
        <columns>sf42_JP_Order_Amount__c</columns>
        <columns>sf42_JP_Gross_Profit__c</columns>
        <columns>sf42_JP_Gross_Profit_Percent__c</columns>
        <filterScope>Everything</filterScope>
        <filters>
            <field>sf42_Delivery_Date__c</field>
            <operation>greaterThan</operation>
            <value>12/31/2020</value>
        </filters>
        <filters>
            <field>ptt_Status__c</field>
            <operation>equals</operation>
            <value>Closed</value>
        </filters>
        <label>Closed Order</label>
        <language>en_US</language>
        <sharedTo>
            <roleAndSubordinates>JapanEmployee</roleAndSubordinates>
        </sharedTo>
    </listViews>
