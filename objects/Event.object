    <validationRules>
        <fullName>Fill_all_mandatory_fields</fullName>
        <active>true</active>
        <errorConditionFormula>AND (RecordTypeId = &quot;012L00000004HtV&quot;,  sf42_de_was_visited__c =true, OR( ISBLANK(text(sf42_de_Visit__c )), ISBLANK(text(sf42_de_Field_Activity__c ))))</errorConditionFormula>
        <errorMessage>Bitte alle Mussfelder ausfüllen:  &quot;Erstbesuch / Wiederholbesuch&quot;und &quot;AD Tätigkeit&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Name_required_if_Customer_Visit</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL( sf42_de_Field_Activity__c, &quot;Customer Visit&quot;) &amp;&amp; ISBLANK(WhoId)</errorConditionFormula>
        <errorDisplayField>WhoId</errorDisplayField>
        <errorMessage>If this is &quot;Customer Visit&quot; then you must fill in Contact or Lead name.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Related_to_required_if_Customer_Visit</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL( sf42_de_Field_Activity__c, &quot;Customer Visit&quot;) &amp;&amp; ISBLANK(WhatId)</errorConditionFormula>
        <errorDisplayField>WhatId</errorDisplayField>
        <errorMessage>If this is &quot;Customer Visit&quot; then you must fill in &quot;Related to&quot; field.</errorMessage>
    </validationRules>
    <webLinks>
        <fullName>Add_project_to_calendar</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <encodingKey>UTF-8</encodingKey>
        <linkType>url</linkType>
        <masterLabel>Add Pproject to Calendar</masterLabel>
        <openType>replace</openType>
        <protected>false</protected>
        <requireRowSelection>false</requireRowSelection>
        <url>/00U/e
?what_id={!sf42_Project__c.Id }
&amp;who_id={!sf42_Project__c.sf42_de_Project_Manager_ExternalId__c}
&amp;evt5=Project%20{!sf42_Project__c.Name}+%20-%20{!sf42_Project__c.sf42_de_Project_Name__c}
&amp;evt15=1
&amp;StartDateTime={!sf42_Project__c.sf42_de_Project_Start__c}
&amp;EndDateTime={!sf42_Project__c.sf42_de_Project_End__c}
&amp;RecordType=012D0000000AyVxIAK
&amp;ent=Event
&amp;retURL=%2F{!sf42_Project__c.Id }</url>
    </webLinks>
