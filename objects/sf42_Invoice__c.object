    <fields>
        <fullName>sf42_consumption_tax__c</fullName>
        <description>Consumption Tax has been changed from 5% to 8% at 1 April 2014.
Consumption tax changed from 8% to 10% from 1st of October 2019.</description>
        <externalId>false</externalId>
        <formula>IF( sf42_consumption_tax_2__c, 
	IF( sf42_Date_of_Invoice_2__c &lt; DATE(2014, 4, 1),sf42_Amount__c * 0.05, 
	 IF( sf42_Date_of_Invoice_2__c &gt;= DATE(2019, 10, 1),sf42_Amount__c * 0.10, sf42_Amount__c * 0.08)), 
	sf42_Amount__c * 0.00)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Consumption Tax Value</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>sf42_controll_field_Amount__c</fullName>
        <externalId>false</externalId>
        <formula>sf42_SALES_MACHINERY_SERVICE__c + Sales_ALI__c + sf42_SALES_CM__c + sf42_SALES_NDT__c +
sf42_SALES_OTHER_PRODUCTS__c + sf42_SALES_OTHER_SERVICE__c + sf42_SALES_PRODUCT_SERVICE_REPAIRS__c + sf42_SALES_TRAINING__c+ sf42_SALES_OTHERS__c + SALES_OTHER_Freight_insurance_etc__c + SALES_OTHER_Rental__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>This amount must be same as total Amount</inlineHelpText>
        <label>controll field Amount</label>
        <precision>18</precision>
