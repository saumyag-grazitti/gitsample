    <validationRules>
        <fullName>Received_Money_is_Invoice_amount</fullName>
        <active>true</active>
        <errorConditionFormula>NOT ( sf42_Amount__c = sf42_Bank_charge__c + sf42_Received_Amount__c + sf42_Amount_loses__c )</errorConditionFormula>
        <errorMessage>Please check your entrey of bank charges, received amount and invoice amount</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>check_Amount</fullName>
        <active>true</active>
        <errorConditionFormula>sf42_Amount__c &lt;&gt; 
BLANKVALUE( sf42_SALES_NDT__c, 0) + BLANKVALUE(sf42_SALES_OTHERS__c, 0) + BLANKVALUE(sf42_SALES_OTHER_PRODUCTS__c, 0) + BLANKVALUE(sf42_SALES_MACHINERY_SERVICE__c, 0) + BLANKVALUE(sf42_SALES_OTHER_SERVICE__c , 0) + BLANKVALUE(sf42_SALES_PRODUCT_SERVICE_REPAIRS__c, 0) + BLANKVALUE(sf42_SALES_TRAINING__c, 0)+
BLANKVALUE(sf42_SALES_CM__c, 0) +
BLANKVALUE(Sales_ALI__c , 0) + BLANKVALUE(SALES_OTHER_Rental__c , 0) + BLANKVALUE(SALES_OTHER_Freight_insurance_etc__c , 0)</errorConditionFormula>
        <errorDisplayField>sf42_Amount__c</errorDisplayField>
        <errorMessage>Please check the entries for the Total Amount and each sales amount if they correct!!!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>received_date_is_entered</fullName>
        <active>true</active>
        <description>When bank is selected than date field cant be empty

AND(OR( ISPICKVAL(sf42_Received_at__c, &quot;Tokyo Mitsubishi UFJ&quot;), ISPICKVAL(sf42_Received_at__c, &quot;Deutsche Bank&quot;)), ISNULL( sf42_Received_on__c ))</description>
        <errorConditionFormula>AND(OR( ISPICKVAL(sf42_Received_at__c, &quot;Tokyo Mitsubishi UFJ&quot;), ISPICKVAL(sf42_Received_at__c, &quot;Deutsche Bank&quot;)), ISNULL( sf42_Received_on__c ))</errorConditionFormula>
        <errorMessage>&quot;No Date or no bank selected!!&quot;</errorMessage>
    </validationRules>
