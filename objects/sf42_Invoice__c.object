    <fields>
        <fullName>sf42_Total_Amount__c</fullName>
        <description>Consumption Tax has been changed from 5% to 8% at 1 April 2014.
Consumption tax changed from 8% to 10% from 1st of October 2019.</description>
        <externalId>false</externalId>
        <formula>IF(sf42_consumption_tax_2__c, IF( sf42_Date_of_Invoice_2__c &lt; DATE(2014, 4, 1),sf42_Amount__c * 1.05,IF( sf42_Date_of_Invoice_2__c &gt;= DATE(2019, 10, 1),sf42_Amount__c * 1.10,sf42_Amount__c * 1.08)),sf42_Amount__c)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Total Amount</label>
        <precision>18</precision>
        <required>false</required>
